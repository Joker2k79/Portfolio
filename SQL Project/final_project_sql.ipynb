   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# функция для вывода sql-таблицы на экран\n",
    "def display_df(sql_query):\n",
    "    df = pd.io.sql.read_sql(sql_query, con = engine)\n",
    "    return df.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "# функция для вывода sql-запроса на экран\n",
    "def display_query(sql_query):\n",
    "    df = pd.io.sql.read_sql(sql_query, con = engine)\n",
    "    return df"
   ]
